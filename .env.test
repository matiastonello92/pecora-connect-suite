# Test Environment Configuration
NODE_ENV=test
VITE_ENVIRONMENT=test
VITE_API_BASE_URL=https://test.managementpn.services

# Supabase Configuration (Test Project)
# TODO: Replace with actual test project credentials
VITE_SUPABASE_URL=https://TEST_PROJECT_ID.supabase.co
VITE_SUPABASE_ANON_KEY=TEST_ANON_KEY
SUPABASE_SERVICE_ROLE_KEY=TEST_SERVICE_ROLE_KEY

# Feature Flags for Test
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_DEBUG_MODE=true
VITE_ENABLE_PERFORMANCE_MONITORING=true

# Test-specific settings
VITE_TEST_DATA_SEED=true
VITE_MOCK_EXTERNAL_SERVICES=true
VITE_FAST_REFRESH=true

# External Services (Test)
RESEND_API_KEY=TEST_RESEND_KEY
RESEND_FROM_EMAIL=test@managementpn.services

# Performance Testing
VITE_PERFORMANCE_BUDGET_QUERY=200
VITE_PERFORMANCE_BUDGET_LOCATION_SWITCH=100
VITE_PERFORMANCE_BUDGET_DASHBOARD=300
VITE_PERFORMANCE_BUDGET_CHAT=150

# Security
VITE_RATE_LIMIT_ENABLED=true
VITE_RATE_LIMIT_REQUESTS=1000
VITE_RATE_LIMIT_WINDOW=3600

# Monitoring
VITE_MONITORING_ENABLED=true
VITE_MONITORING_ENDPOINT=https://test.managementpn.services/api/monitoring